#include<stdio.h>
#include<limits.h>
#include<math.h>

void main()
{
	int a= 0x2F;
	int b= 023;
	char c= '0';
	char d= 'a';
	char e= '\o23';
	char f='\x23';
	printf("%i\n", a);
	printf("%d\n",SHRT_MAX);
	printf("%d\n",s);
	printf("%d\n",CHAR_MAX);
	printf("%d\n",b);
	printf("%d\n %c\n",c,e);
	printf("f=%c",f);
}

